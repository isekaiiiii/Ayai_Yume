						chr:TranslateBy(hum.MoveDirection) 
					kết thúc 
				kết thúc 

			kết thúc) 
		kết thúc kết thúc 
	kết 
thúc) 
mine.MouseButton1Down:connect(function() 
	nếu tốc độ == 1 thì 
		tốc độ.Text = 'không được nhỏ hơn 1' 
		wait(1) 
		speed.Text = tốc độ 
	else 
		speed = tốc độ - 1 
		speed.Text = tốc độ 
		nếu nowe == true thì 
			tpwalking = false 
			đối với i = 1, tốc độ thực hiện 
				spawn(function() 

					cục bộ hb = game:GetService("RunService").Heartbeat	 


